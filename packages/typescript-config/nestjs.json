{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "NestJS",
    "extends": "./base.json",
    "compilerOptions": {
        "target": "ES2021",
        "module": "commonjs",
        "moduleResolution": "node",
        "lib": [
            "ES2021"
        ],
        "outDir": "./dist",
        "rootDir": "./src",
        "declaration": true,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "incremental": true,
        "noEmit": false,
        "strictPropertyInitialization": false
    }
}